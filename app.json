{
  "expo": {
    "name": "Valorant Account Viewer",
    "slug": "vav-app-android",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/valorantlogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/valorantlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#131313"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/valorantlogo.png",
        "backgroundColor": "#131313"
      },
      "package": "com.zetadev.vav",
      "googleServicesFile": "./google-services.json",
      "permissions": ["INTERNET"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/valorantlogo.png",
          "color": "#ff8484"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "0a0d71f3-e7e0-4088-87bd-e29643560b48"
      }
    }
  }
}
